@model IEnumerable<WebApplication2.Models.Stock>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Stkid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StkDesc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Stkid)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.StkDesc)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Stkid">Details</a> |
            </td>
        </tr>
}
    </tbody>
</table>
