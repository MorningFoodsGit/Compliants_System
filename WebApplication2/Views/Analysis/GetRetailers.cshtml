@model WebApplication2.Models.ViewModels.AnalysisViewModel

<table class="table table-striped mb-0">
    <thead>
        <tr>
            <th>#</th>
            <th>Retailer</th>
        </tr>
    </thead>
    <tbody>
        @for (var i = 0; i < Model.Retailers.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model.Retailers[i]</td>
            </tr>
        }
    </tbody>
</table>
